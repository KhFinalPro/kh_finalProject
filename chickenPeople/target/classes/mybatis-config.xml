		<typeAlias type="com.kh.chickenPeople.message.model.vo.Message" alias="Message"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="/mappers/member-mapper.xml"/>
		<mapper resource="/mappers/message-mapper.xml"/>
	</mappers>
</configuration>
